{
  "name": "minimalist-typewriter",
  "version": "1.18.0",
  "description": "A very simple and performant package for typing out headings.",
  "main": "dist/react.js",
  "repository": "https://github.com/TeleworkInc/typewriter.git",
  "author": "Christian Lewis <ctjl96@gmail.com>",
  "license": "SEE LICENSE IN .LICENSE/LICENSE",
  "private": false,
  "scripts": {
    "build:prod": "webpack --config ./webpack/production.config.babel.js",
    "build:dev": "webpack --config ./webpack/development.config.babel.js",
    "build:core": "google-closure-compiler -O ADVANCED --js ./src/core.js --js_output_file ./dist/browser.js",
    "build:react": "google-closure-compiler -O ADVANCED --js ./src/react.js --js_output_file ./dist/react.js",
    "build:closure": "yarn build:core && yarn build:react"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@babel/runtime": "^7.8.7",
    "@testing-library/react": "^9.5.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-env": "^1.7.0",
    "jest": "^25.1.0",
    "lolex": "^6.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "object-assign": "^4.1.1",
    "regenerator-runtime": "^0.13.5"
  }
}
